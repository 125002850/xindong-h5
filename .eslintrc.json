{
    "extends": ["lcy-vue"],
    "globals": {
        "uni": true,
        "plus": true,
        "wx": true
    },
    "rules": {
        "no-inline-comments": 0,
        "no-irregular-whitespace": 0,
        "no-unused-expressions": 0,
        "vue/multi-word-component-names": 0,
        "vue/no-v-for-template-key": 0,
        "vue/no-v-model-argument": 0
    }
}
