{
  "name": "vite-vue3-h5",
  "type": "module",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@9.1.0",
  "author": "lincenying <lincenying@qq.com>",
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "del": "sh delete-ts.sh",
